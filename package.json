{
    "name": "nucleus-automation",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test:cross-platform": "npx codeceptjs run ./src/tests/codecept/cross_platform_test.ts --steps",
        "test:performance": " k6 run ./src/tests/performance/test.ts",
        "bdd": "npx codeceptjs run --features --grep '@Login' --verbose --debug",
        "bdd:web:debug": "PWDEBUG=1 npx codeceptjs run --features --grep '@SchedulingWeb' --verbose --debug --config=./config/codecept.web.test.conf.ts",
        "bdd:mobile": "npx codeceptjs run --features --grep '@LaunchNucelusApp' --verbose --debug --config=./config/codecept.mobile.test.conf.ts",
        "bdd:main-app": "npx codeceptjs run --features --grep '@LaunchNucelusApp' --verbose --debug --config=./config/codecept.main.app.test.conf.ts",
        "bdd:overnight-app": "npx codeceptjs run --features --grep '@ON-022' --verbose --debug --config=./config/codecept.overnight.app.test.conf.ts",
        "bdd:web": "npx codeceptjs run --features --grep '@Regression' --verbose --debug --config=./config/codecept.web.test.conf.ts",
        "bdd:web:smoke": "npx codeceptjs run --features --grep '@Smoke' --verbose --debug --config=./config/codecept.web.test.conf.ts",
        "bdd:web:dev": "npx codeceptjs run --features --grep '@CC-TEST' --verbose --debug --config=./config/codecept.web.test.conf.ts",
        "bdd:web:sanity:parallel": "npx codeceptjs run-workers 4 --features --grep '@SanityParallel' --verbose --debug --config=./config/codecept.web.test.conf.ts",
        "bdd:web:sanity:single-thread": "npx codeceptjs run --features --grep '@SanitySingleThread' --verbose --debug --config=./config/codecept.web.test.conf.ts",
        "bdd:web:sanity": "npx codeceptjs run --features --grep '@SanityWeb' --verbose --debug --config=./config/codecept.web.test.conf.ts",
        "bdd:api:sanity": "npx codeceptjs run --features --grep '@SanityApi' --verbose --debug --config=./config/codecept.backend.test.conf.ts",

        "bdd:web:sanity:clean-up": "npx codeceptjs run --features --grep '@CC-Clean' --verbose --debug --config=./config/codecept.web.test.conf.ts",

        "bdd:bot": "npx codeceptjs run --features --grep '@(SanityApi|SanityWeb)' --verbose --debug --config=./config/codecept.bot.conf.ts",
        "bdd:api": "npx codeceptjs run --features --grep '@DeviceUx' --verbose --debug --config=./config/codecept.backend.test.conf.ts",
        "report": "allure serve output",
        "generate-report": "allure generate output --clean -o allure-report",
        "generate-report:single-file": "allure generate output --clean --single-file",
        "publish-report": "ts-node ./src/utils/azure/uploadReport.ts ",
        "open-generated-report": "allure open allure-report",
        "trace": "npx playwright show-trace path/to/trace.zip",
        "lint": "npx eslint src test",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "npx prettier src test --check",
        "prettier:fix": "npm run prettier -- --write",
        "format": "npm run prettier:fix && npm run lint:fix",
        "docker:build": "docker build -t nucleuscare-automation .",
        "docker:run": "docker run -d -p 3000:3000 nucleuscare-automation",
        "postinstall": "patch-package && npx playwright install && npx playwright install-deps",
        "encrypt-credentials": "ts-node ./src/utils/data/encrypt.ts src/data/backend/api/AuthAccounts.secret.json",
        "decrypt-credentials": "ts-node ./src/utils/data/decrypt.ts ./src/data/backend/api/AuthAccounts.secret.json.enc",
        "test-email-parserer": "ts-node ./src/utils/readEmails.ts",
        "test-gmail-parserer": "ts-node ./src/utils/google/index.ts",
        "test-database-connection": "ts-node src/utils/helpers/DbConnector.ts",
        "encrypt-google-credentials": "ts-node ./src/utils/data/encrypt.ts src/utils/google/credentials/credentials.secret.json",
        "decrypt-google-credentials": "ts-node ./src/utils/data/decrypt.ts src/utils/google/credentials/credentials.secret.json.enc",
        "encrypt-google-token": "ts-node ./src/utils/data/encrypt.ts src/utils/google/credentials/token.secret.json",
        "decrypt-google-token": "ts-node ./src/utils/data/decrypt.ts src/utils/google/credentials/token.secret.json.enc"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "./**/*.{js,jsx,ts,tsx}": [
            "npx prettier --write",
            "eslint **/*.ts --fix-dry-run"
        ]
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@azure/storage-blob": "^12.23.0",
        "@codeceptjs/allure-legacy": "^1.0.2",
        "@codeceptjs/configure": "^0.10.0",
        "@codeceptjs/examples": "^1.2.1",
        "@codeceptjs/ui": "^0.7.0",
        "@faker-js/faker": "^8.3.1",
        "@types/lodash": "^4.14.199",
        "@types/node": "^20.10.8",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^8.0.1",
        "codeceptjs": "^3.5.11",
        "dotenv": "^16.3.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-codeceptjs": "^1.3.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^4.2.3",
        "lint-staged": "^10.5.4",
        "mime": "^4.0.3",
        "mime-types": "^2.1.35",
        "patch-package": "^8.0.0",
        "playwright": "^1.40.1",
        "postinstall-postinstall": "^2.1.0",
        "prettier": "^3.2.4",
        "webdriverio": "^6.12.1"
    },
    "dependencies": {
        "@google-cloud/local-auth": "^3.0.1",
        "@types/k6": "^0.49.0",
        "@types/moment": "^2.13.0",
        "chai": "^4.3.8",
        "cli-progress": "^3.12.0",
        "codeceptjs-chai": "^2.3.5",
        "fs": "^0.0.1-security",
        "googleapis": "^144.0.0",
        "imap": "^0.8.19",
        "lodash": "^4.17.21",
        "mailparser": "^3.7.1",
        "moment": "^2.30.1",
        "mssql": "^11.0.1",
        "path": "^0.12.7",
        "pixelmatch": "^5.3.0",
        "pngjs": "^7.0.0",
        "puppeteer": "^22.15.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}
